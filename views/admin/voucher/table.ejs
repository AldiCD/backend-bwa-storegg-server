<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No</th>
      <th>Name Game</th>
      <th>Kategory</th>
      <th>Nominal</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <%for(let i = 0;i < voucher.length;i++){%>
    <tr>
      <td><%= i +1%></td>
      <td><%= voucher[i].name%></td>
      <td><%= voucher[i]?.category?.name??"-"%></td>
      <td>
        <% voucher[i].nominals.forEach(nominal => {%>
        <span class="badge badge-warning">
          <%= nominal.coinName %> <%= nominal.coinQuantity %>
        </span>
        <%})%>
      </td>
      <td>
          <%if(voucher[i].status === "Y"){%> 
            <span class="badge badge-succes">Hidup</span>
            <%} else {%>
            <span class="badge badge-danger">Mati</span>
             <%}%>
      </td>

      <td>
        <form action="/voucher/status/<%=voucher[i]._id%>?_method=PUT" method="post">
          <button class="btn btn-primary btn-sm" style="display:inline-block">
            <i class="fas fa-folder sm"></i>
            <%if(voucher[i].status === "Y"){%> 
              Mati 
            <%} else {%> 
              Hidup 
            <%}%>
          </button>

          <a href="/voucher/edit/<%=voucher[i]._id%>" style="display:inline-block" class="btn btn-info btn-sm" >
            <i class="fas fa-pencil-alt"></i>Edit
          </a>
        </form>
        <form action="/voucher/delete/<%=voucher[i]._id%>?_method=DELETE"
          method="post" style="display:inline-block">
          <button class="btn btn-danger btn-sm" ">
            <i class="fas fa-trash"></i>Hapus
          </button>
        </form>
      </td>
    </tr>
    <%}%>
  </tbody>
</table>
